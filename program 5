// Take 5 float-numbers as input and find the average of them.

#include<stdio.h>
int main(){

    float a,b,c,d,e,avg;

    printf("Enter 5 float numbers : \n");
    scanf("%f %f %f %f %f", &a,&b,&c,&d,&e);

    avg = (a+b+c+d+e)/5;
    printf("The avg of the numbers is : %f\n",avg);
     
     return 0;

}
